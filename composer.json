{
    "name": "flaphl/contracts",
    "description": "abstractions pulled from the Flaphal elements.",
    "license": "MIT",
    "type": "library",
    "keywords": [ "flaphl", "contracts"],
    "authors": [
        {
            "name": "Jade Phyressi",
            "email": "jade@flaphl.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "psr/cache": "^3.0",
        "psr/container": "^2.0",
        "psr/event-dispatcher": "^1.0"
    },
    "conflict": {
        "ext-psr": "<1.1|>=2.0"
    },
    "replace": {
        "flaphl/deprecation-contracts": "self.version",
        "flaphl/cache-contracts": "self.version",
        "flaphl/event-dispatcher-contracts": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Flaphl\\Contracts\\": ""},
        "files": [ "Deprecation/functions.php" ],
        "exclude-from-classmap": ["**/Tests/"]
    },
    "minimum-stability": "dev",
    "prefer-stable": false
}